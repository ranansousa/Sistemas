<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="br.com.pempec.finance.models">
    <class name="BancoModel" table="BANCO">
        <id column="ID_BANCO" name="id"/>
        <property column="NOME_BANCO" name="nomeBanco"/>
        <property column="CODIGO_BANCO" name="codigoBanco"/>
        <property column="SIGLA_BANCO" name="siglaBanco"/>

    </class>
</hibernate-mapping>
